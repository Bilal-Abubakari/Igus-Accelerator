{
  "name": "@igus-accelerator-injection-molding-configurator/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:frontend": "yarn nx lint iaimc-frontend && yarn nx serve iaimc-frontend",
    "start:backend": "yarn nx lint iaimc-backend && yarn nx serve iaimc-backend",
    "start:all": "nx run-many --target=serve --projects=iaimc-frontend,iaimc-backend --parallel",
    "lint": "nx run-many --target=lint --no-cloud --all",
    "lint:fix": "nx run-many --target=lint -- --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "prepare": "husky",
    "build:frontend": "nx build frontend",
    "build:backend": "nx build backend",
    "test:frontend": " nx test iaimc-frontend --no-cloud",
    "test:frontend-c": " nx test iaimc-frontend --no-cloud --coverage ",
    "test:backend": " nx test iaimc-backend --no-cloud",
    "test:backend-c": " nx test iaimc-backend --no-cloud --coverage ",
    "test:all": "nx run-many --target=test --no-cloud --all",
    "test:all-c": "nx run-many --target=test --no-cloud --coverage --all --skip-nx-cache",
    "render:build": "npx yarn install --frozen-lockfile --ignore-scripts && npx yarn nx build iaimc-backend --parallel=1 --skip-nx-cache"
  },
  "private": true,
