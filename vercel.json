{
  "version": 2,
  "name": "molding-configurator",
  "builds": [
    {
      "src": "apps/iaimc-backend/src/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "apps/iaimc-backend/src/main.js"
    }
  ],
  "buildCommand": "npx nx build iaimc-backend",
  "installCommand": "npm install",
  "outputDirectory": "dist/apps/iaimc-backend"
}
